def getInteger(): # we will go down and intend code
    result = int(input("Please enter any no : "))
    return result


#Adding a parameters
def getInteger(promt): # we will go down and intend code
    result = int(input(promt))
    return result

def Main():
    print 'Started'
    output = getInteger("Please enter an int: ")
    output2 = getInteger("Another int  : ")
    print output

#MAIN function
