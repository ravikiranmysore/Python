#Iterations
def Main():
    for step in range(5):
        print step

    words = ['cat','bat','hat','sat']
    for word in words:
        print word

    num = 0
    while num <= 0:
        num = int(input('Please enter a +ve integer : '))

    while True:
        print 'hello'

if __name__ == "__main__":
    Main()

